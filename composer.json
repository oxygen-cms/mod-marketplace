{
    "name": "oxygen/mod-marketplace",
    "description": "A Marketplace for Oxygen",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "c.chamberlain@iinet.net.au"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "oxygen/core": "~0.5.0",
        "oxygen/preferences": "~0.2.0",
        "illuminate/support": "~6.0",
        "michelf/php-markdown": "~1.4",
        "composer/composer": "dev-progress",
        "guzzlehttp/guzzle": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "OxygenModule\\Marketplace\\": "src/"
        }
    },
    "extra": {
        "title": "Oxygen Module - Marketplace",
        "readme": "oxygen.md",
        "changes": "changes.md",
        "icon": "icon.png",
        "providers": [
            {
                "name": "Marketplace",
                "class": "OxygenModule\\Marketplace\\MarketplaceServiceProvider",
                "description": "Adds the Marketplace"
            }
        ]
    }
}
