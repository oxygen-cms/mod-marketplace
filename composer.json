{
    "name": "oxygen/marketplace",
    "description": "A Marketplace for Oxygen",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "c.chamberlain@iinet.net.au"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "oxygen/core": "~0.2.0",
        "composer/composer": "@dev",
        "illuminate/support": "4.2.*"
    },
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "michelf/php-markdown": "~1.4",
        "composer/composer": "dev-master",
        "guzzlehttp/guzzle": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\Marketplace\\": "src/"
        }
    },
    "extra": {
        "title": "Oxygen Marketplace",
        "readme": "oxygen.md",
        "changes": "changes.md",
        "icon": "icon.png",
        "providers": [
            {
                "name": "Marketplace",
                "class": "Oxygen\\Marketplace\\MarketplaceServiceProvider",
                "description": "Adds the Marketplace"
            }
        ]
    }
}
